<nav aria-label="Page navigation">
    <ul class="pagination">
        {% for letter in 'A' .. 'Z' %}
            {% set class='' %}
            {% if letter in letters %}
                {% set class='active' %}
            {% endif %}
            <li class="{{ class }}"><a href="?alpha={{ letter }}">{{ letter }}</a></li>
        {% endfor %}
    </ul>
</nav>
